{
  "notes": "like diet_main_sm_reg_v0.0.2.0, but regularization const is 0.01 (factor 2 higher) + hist summaries removed too",
  "model_base_path": "s3://mobileye-habana/mobileye-team-stereo/models",
  "model_params": {
    "arch": {
      "name": "stereo/sdsv3_v0.5.5_stereo_corr_diet",
      "kwargs": {
        "batch_norm": 0,
        "min_Z": 1.0,
        "max_Z": 1000.0,
        "regularization" : "l2"
      },
      "map_func_kwargs": {
        "main": {
          "num_steps_corr": 45,
          "min_Z_corr": 2.0,
          "max_Z_corr": 80.0,
          "min_delta_Z_corr": 0.2
        }
      }
    },
    "loss": {
      "name": "stereo/lidar+short+phot_v1.4.1.6",
      "kwargs": {
        "measure": "ssim",
        "alpha_ooi": 0.0,
        "alpha_lidar": 10.0,
        "alpha_lidar_short": 0.25,
        "reg_constant" : 0.01
      },
      "map_func_kwargs": {
        "consts_path": "s3://mobileye-habana/mobileye-team-stereo/data/tf_datasets/v2.2/blur_kernels.npz",
        "main": {
          "blur_kernels_names": [
            "frontCornerLeft_to_main",
            "frontCornerRight_to_main",
            "parking_front_to_main"
          ]
        }
      }
    },
    "learning_rate": 1e-4,
    "train_op": "AdamOptimizer",
    "init": {
      "restore_conf": {},
      "global_step": 0,
      "random_seed": 6252
    },
    "eval_steps": 5000,
    "summary_steps": 200,
    "strings_to_summarize": [
      "features/sample_id"
    ],
    "scalars_to_summarize": [
      "loss/loss_phot",
      "loss/loss_lidar",
      "loss/loss_lidar_short",
      "loss/loss_reg",
      "loss/loss"
    ],
    "images_to_summarize": [
      "features/I_cntr",
      "features/I_srnd_0",
      "features/I_srnd_1",
      "features/I_srnd_2",
      "features/im_mask",
      "features/im_lidar_inv",
      "features/im_lidar_short_inv",
      "loss/I_warp_cntr_0",
      "loss/I_warp_cntr_1",
      "loss/I_warp_cntr_2",
      "arch/out"
    ],
    "reverse_image_summaries": true,
    "with_model_scoping": true,
    "scalars_to_log": [
      "loss/loss_phot",
      "loss/loss_lidar",
      "loss/loss_lidar_short",
      "loss/loss_reg",
      "loss/loss"
    ]
  },
  "data_params": {
    "datasets": {
      "main":  "main_with_pf.v2.2"
    }
  },
  "num_iters": 300000,
  "num_epochs": 70,
  "test_every": 100,
  "save_every": 10000
}
