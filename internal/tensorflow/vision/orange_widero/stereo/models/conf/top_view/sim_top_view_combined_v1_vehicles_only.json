{
  "notes": "",
  "model_base_path": "s3://mobileye-habana/mobileye-team-stereo/models",
  "model_params": {
    "arch": {
      "name": "top_view/top_view_peds_vehicles_multi_frame",
      "kwargs": {
        "regularization" : "l2",
        "num_of_categories": 1
      }
    },
    "loss": {
      "name": "top_view/sim_top_view_loss_per_category",
      "kwargs": {
        "reg_constant" : 0.01,
        "num_of_categories": 1,
        "categories_map": [-1, -1, 0, -1, -1, -1, -1],
        "categories_from_top_view": [4, 5]
      },
      "map_func_kwargs": {
          "sim_top_view_v1":{
            "use_top_view_cam_label": true
          }
      }
    },
    "learning_rate": 5e-5,
    "train_op": "AdamOptimizer",
    "init": {
      "restore_conf": {},
      "global_step": 0,
      "random_seed": 6252
    },
    "eval_steps": 100,
    "summary_steps": 200,
    "reverse_image_summaries": true,
    "with_model_scoping": true,
    "strings_to_summarize": [
      "features/sample_id"
    ],
    "scalars_to_summarize": [
      "loss/loss",
      "loss/scalar_loss_*"
    ],
    "images_to_summarize": [
      "arch/input_image",
      "loss/rgb_label",
      "loss/*_out",
      "loss/image_*loss"
    ],
    "scalars_to_log": [
      "loss/loss",
      "loss/loss_reg"
    ]
  },
  "data_params": {
    "datasets": {"sim_top_view_v1":  "sim_top_view_v1"},
    "ds_version": "sim_v1"
  },
  "num_iters": 300000,
  "num_epochs": 70,
  "test_every": 100,
  "save_every": 10000
}